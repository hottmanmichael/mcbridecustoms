<%- include('partials/head') %>



    <body>
        <%- include('partials/navbar') %>
        <h1>Contact</h1>
        <div class="container">
            <form class="form-group"action="/contact" method="post">
                <div class="col-md-6">
                    <div class="form-group col-md-12">
                        <label for="name">Name</label>
                        <input name="name" type="text" class="form-control" id="name" >
                    </div>
                    <div class="form-group col-md-12">
                        <label for="email">Email</label>
                        <input name="email" type="email" class="form-control" id="email" >
                    </div>
                    <div class="form-group col-md-12">
                        <label for="subject">Subject</label>
                        <input name="subject" type="text" class="form-control" id="subject">
                    </div>
                </div>
                <div class="col-md-6">

                    <div class="form-group col-md-12">
                        <label for="message">Message</label>
                        <textarea class="contact-message" name="text_body" placeholder="Type your message here"></textarea>
                    </div>
                </div>

                <button class="btn btn-success btn-lg" type="submit" name="button">Send Email</button>
            </form>
        </div>

        <%- include('partials/footer') %>
    </body>




<%- include('partials/foot') %>
